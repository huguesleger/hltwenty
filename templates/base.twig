{% block html_head_container %}

{% include 'html-header.twig' %}
{% block head %}
{% endblock %}
</head>
{% endblock %}

<body class="scroll-content {{body_class}}">
	<div class="circle-cursor circle-cursor--inner"></div>
	<div class="circle-cursor circle-cursor--view"></div>
	<div class="circle-cursor circle-cursor--outer">
	</div>
	{% block loader %}
	<div class="loader">
		<div class="loader-content">
			<div class="progress-circle">
				<svg height="130" width="130">
					<circle class="circle-line" cx="50%" cy="50%" r="62" fill="rgba(0,0,0,0)" stroke-width="2"
						strokeDashoffset="400" stroke-dasharray="200" stroke="#3423A6"/>
				</svg>
				<div class="loader-circle">
					<div class="loader-logo"></div>
				</div>
			</div>
		</div>
	</div>
	{% endblock %}
	{% block header %}
	
		<header class="header">
			<div class="content-header">
				<a class="logo hoverable" href="{{site.url}}" rel="home"><img src="{{custom_logo_url}}" />
					<span class="title-site">{{site.name}}</span>
				</a>
				<div class="btn-main hoverable">
					<span class="btn-fill"></span>
				</div>
			</div>
		</header>
		<nav id="nav-main" class="main" role="navigation">
			<div class="wrap-grid expanded">
				<div class="half-line before-lines">
					<div class="cover-segment"></div>
				</div>
				<div class="wrap-lines container">
					<div class="row no-gutters">
						<div class="col-3 liner">
							<div class="cover-segment"></div>
						</div>
						<div class="col-3 liner">
							<div class="cover-segment"></div>
						</div>
						<div class="col-3 liner">
							<div class="cover-segment"></div>
						</div>
						<div class="col-3 liner">
							<div class="cover-segment"></div>
						</div>
					</div>
				</div>
				<div class="half-line after-lines">
					<div class="cover-segment"></div>
				</div>
			</div>
				<div class="container px-0">
					{% include "menu.twig" with {'items': menu.get_items} %}
				</div>
		</nav>	
	{% endblock %}
	<div class="app" id="app">
	{% block content %}
	{% endblock %}
	{% block footer %}
	<footer id="footer" class="footer">
		{% include 'footer.twig'with {'items': menu_footer.get_items} %}
	</footer>
	{{ function('wp_footer') }}
	{% endblock %}
	{% block javascripts %}
	{% endblock %}
</body>

</html>